{
  "manifest_version": 2,
  "name": "Full Window YT",
  "version": "0.0.1",
  "icons": {
    "96": "fwyt-96-red.png",
    "48": "fwyt-48-red.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/watch*"],
      "js": ["script.js"],
      "css": ["styles.css"]
    }
  ],
  "browser_action": {
    "default_icon": "fwyt-48-red.png"
  }
}
